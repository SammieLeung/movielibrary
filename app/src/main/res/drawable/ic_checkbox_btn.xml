<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_focused="true">
        <bitmap android:src="@mipmap/ic_checkbox_checked_f"></bitmap>
    </item>
    <item android:state_checked="true" android:state_focused="false">
        <bitmap android:src="@mipmap/ic_checkbox_checked"></bitmap>
    </item>
    <item android:state_focused="true">
        <bitmap android:src="@mipmap/ic_checkbox_f"></bitmap>
    </item>
    <item>
        <bitmap android:src="@mipmap/ic_checkbox"></bitmap>
    </item>
</selector>