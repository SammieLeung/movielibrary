<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/c3028bbfb" />
            <stroke
                android:width="1px"
                android:color="@color/cffffff" />
            <size
                android:width="120dp"
                android:height="120dp" />

        </shape>
    </item>
    <item android:top="30dp" android:bottom="30dp" android:left="30dp" android:right="30dp">
        <shape
            android:shape="oval"
            android:useLevel="false">
            <solid android:color="@color/c28bbfb" />
            <stroke
                android:width="1px"
                android:color="@color/cffffff" />

        </shape>
    </item>

</layer-list>
