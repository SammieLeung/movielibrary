    "views": [
      {
        "viewName": "movie_dataview",
        "createSql": "CREATE VIEW `${VIEW_NAME}` AS SELECT M.id,M.movie_id,M.title,M.pinyin,M.poster,M.ratings,M.year,MVCF__VF__SD.source,MVCF__VF__SD.path,MVCF__VF__SD.dir_path,MVCF__VF__SD.device_id,genre_name,M.add_time,M.last_playtime,M.is_favorite FROM movie AS M JOIN (SELECT MVCF.id,MVCF.path,MVCF.source,VF__SD__DEV.dir_path,VF__SD__DEV.device_id FROM movie_videofile_cross_ref AS MVCF JOIN (SELECT VF__SD.path,VF__SD.dir_path,DEV.id AS device_id FROM device AS DEV JOIN (SELECT VF.path,SD.path AS dir_path,SD.device_path FROM videofile AS VF JOIN scan_directory AS SD ON VF.dir_path=SD.path) AS VF__SD ON VF__SD.device_path=DEV.path) AS VF__SD__DEV ON MVCF.path=VF__SD__DEV.path) AS MVCF__VF__SD ON MVCF__VF__SD.id = M.id LEFT OUTER  JOIN (SELECT MGCF.id,G.name AS genre_name FROM genre AS G JOIN movie_genre_cross_ref AS MGCF ON MGCF.genre_id = G.genre_id) AS MGCF__G ON MGCF__G.id = M.id"
      },
      {
        "viewName": "unrecognizedfile_dataview",
        "createSql": "CREATE VIEW `${VIEW_NAME}` AS SELECT VF.vid,VF.filename,VF.keyword,VF.path,VF.last_playtime,SD__DEV.dir_path,SD__DEV.device_id,SD__DEV.device_path FROM videofile AS VF JOIN (SELECT SD.path AS dir_path,DEV.id AS device_id,DEV.path AS device_path FROM device AS DEV JOIN scan_directory AS SD ON SD.device_path=DEV.path) AS SD__DEV ON VF.dir_path=SD__DEV.dir_path"
      }
    ],
